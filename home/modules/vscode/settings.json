{
  "update.mode": "none",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "editor.formatOnSave": true,
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.formatOnSaveMode": "modifications",
  "editor.tabCompletion": "off",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "security.workspace.trust.enabled": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "extensions.ignoreRecommendations": true,
  "editor.cursorBlinking": "solid",
  "editor.lineNumbers": "relative",
  "git.autofetch": true,
  "vim.useCtrlKeys": true,
  "vim.autoSwitchInputMethod.enable": true,
  "vim.autoSwitchInputMethod.defaultIM": "1",
  "vim.autoSwitchInputMethod.obtainIMCmd": "/run/current-system/sw/bin/fcitx5-remote",
  "vim.autoSwitchInputMethod.switchIMCmd": "/run/current-system/sw/bin/fcitx5-remote -t {im}",
  "vim.leader": "<space>",
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.camelCaseMotion.enable": true,
  "vim.easymotion": true,
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "g",
        "s"
      ],
      "commands": [
        "git.stageChange"
      ]
    },
    {
      "before": [
        "<leader>",
        "m"
      ],
      "commands": [
        "bookmarks.toggle"
      ]
    },
    {
      "before": [
        "<leader>",
        "bl"
      ],
      "commands": [
        "bookmarks.list"
      ]
    },
    {
      "before": [
        "]",
        "p"
      ],
      "commands": [
        "editor.action.marker.prevInFiles"
      ]
    },
    {
      "before": [
        "[",
        "p"
      ],
      "commands": [
        "editor.action.marker.nextInFiles"
      ]
    },
    {
      "before": [
        "[",
        "b"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
    {
      "before": [
        "]",
        "b"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "<C-n>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    {
      "before": [
        "leader",
        "b",
        "d"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "leader",
        "b",
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "leader",
        "h",
        "c"
      ],
      "after": [
        ":",
        "n",
        "o",
        "h",
        "enter"
      ]
    },
    {
      "before": [
        "<C-d>"
      ],
      "after": [
        "2",
        "5",
        "g",
        "j",
        "z",
        "z"
      ]
    },
    {
      "before": [
        "<C-u>"
      ],
      "after": [
        "2",
        "5",
        "g",
        "k",
        "z",
        "z"
      ]
    }
  ],
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "git.confirmSync": false,
  "nix.serverPath": "nil",
  "nix.enableLanguageServer": true,
  "nix.serverSettings": {
    "nil": {
      "formatting": {
        "command": [
          "nixpkgs-fmt"
        ]
      }
    }
  },
  "vim.foldfix": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "githubPullRequests.fileListLayout": "flat",
  "editor.unicodeHighlight.allowedLocales": {
    "zh-hans": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Nord",
  "metals.serverVersion": "1.1.0",
  "python.formatting.provider": "black",
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.completeFunctionParens": true,
  "python.analysis.autoFormatStrings": true,
  "window.zoomLevel": -1,
  "bookmarks.label.suggestion": "useWhenSelected",
  "bookmarks.showCommandsInContextMenu": true,
  "latex-workshop.latex.autoBuild.run": "onSave",
  "latex-workshop.latex.recipe.default": "XeLaTeXmk",
  "latex-workshop.latex.recipes": [
      {
          "name": "XeLaTeXmk",
          "tools": [
              "XeLaTeXmk"
          ]
      },
      {
          "name": "pdfLaTeXmk",
          "tools": [
              "pdfLaTeXmk"
          ]
      },
      {
          "name": "LuaLaTeXmk",
          "tools": [
              "LuaLaTeXmk"
          ]
      },
      {
          "name": "LaTeXmk-DVIPDFMx",
          "tools": [
              "LaTeXmk-DVIPDFMx"
          ]
      },
      {
          "name": "upLaTeXmk-DVIPDFMx",
          "tools": [
              "upLaTeXmk-DVIPDFMx"
          ]
      }
  ],
  "latex-workshop.latex.tools": [
      {
          "args": [
              "-xelatex",
              "-synctex=1",
              "-shell-escape",
              "-interaction=nonstopmode",
              "-file-line-error",
              "%DOC%"
          ],
          "command": "latexmk",
          "name": "XeLaTeXmk"
      },
      {
          "args": [
              "-pdflatex",
              "-synctex=1",
              "-shell-escape",
              "-interaction=nonstopmode",
              "-file-line-error",
              "%DOC%"
          ],
          "command": "latexmk",
          "name": "pdfLaTeXmk"
      },
      {
          "args": [
              "-lualatex",
              "-synctex=1",
              "-shell-escape",
              "-interaction=nonstopmode",
              "-file-line-error",
              "%DOC%"
          ],
          "command": "latexmk",
          "name": "LuaLaTeXmk"
      },
      {
          "args": [
              "-e",
              "$dvipdf='dvipdfmx %O -o %D %S'",
              "-latex",
              "-pdfdvi",
              "-synctex=1",
              "-shell-escape",
              "-interaction=nonstopmode",
              "-file-line-error",
              "%DOC%"
          ],
          "command": "latexmk",
          "name": "LaTeXmk-DVIPDFMx"
      },
      {
          "args": [
              "-e",
              "$dvipdf='dvipdfmx %O -o %D %S'",
              "-latex=uplatex",
              "-pdfdvi",
              "-synctex=1",
              "-shell-escape",
              "-interaction=nonstopmode",
              "-file-line-error",
              "%DOC%"
          ],
          "command": "latexmk",
          "name": "upLaTeXmk-DVIPDFMx"
      }
  ],
  "latex-workshop.view.pdf.viewer": "tab"
}
